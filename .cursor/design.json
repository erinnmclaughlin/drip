{
    "designSystem": {
      "name": "API Documentation Dark Theme",
      "description": "Dark-themed design system for API documentation interface with Swagger/OpenAPI styling",
      "version": "1.0.0"
    },
    "colorPalette": {
      "background": {
        "primary": "#1a1a1a",
        "secondary": "#242424",
        "tertiary": "#2d2d2d",
        "codeBlock": "#0d0d0d",
        "hover": "#333333"
      },
      "text": {
        "primary": "#ffffff",
        "secondary": "#b3b3b3",
        "muted": "#808080",
        "link": "#61affe",
        "code": "#e6e6e6"
      },
      "accent": {
        "success": "#49cc90",
        "error": "#f93e3e",
        "warning": "#fca130",
        "info": "#61affe",
        "get": "#61affe",
        "post": "#49cc90",
        "put": "#fca130",
        "delete": "#f93e3e"
      },
      "tags": {
        "beta": "#ff6b6b",
        "new": "#4ecdc4"
      },
      "borders": {
        "default": "#404040",
        "light": "#333333",
        "focus": "#61affe"
      }
    },
    "typography": {
      "fontFamilies": {
        "primary": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
        "monospace": "'SF Mono', Monaco, 'Cascadia Code', 'Roboto Mono', Consolas, monospace"
      },
      "fontSizes": {
        "xs": "11px",
        "sm": "13px",
        "base": "14px",
        "md": "16px",
        "lg": "18px",
        "xl": "24px",
        "2xl": "32px"
      },
      "fontWeights": {
        "normal": 400,
        "medium": 500,
        "semibold": 600,
        "bold": 700
      },
      "lineHeights": {
        "tight": 1.2,
        "normal": 1.5,
        "relaxed": 1.8
      }
    },
    "spacing": {
      "unit": 4,
      "scale": {
        "xs": "4px",
        "sm": "8px",
        "md": "16px",
        "lg": "24px",
        "xl": "32px",
        "2xl": "48px",
        "3xl": "64px"
      }
    },
    "layout": {
      "sidebar": {
        "width": "260px",
        "background": "#1a1a1a",
        "borderRight": "1px solid #404040",
        "padding": "16px"
      },
      "mainContent": {
        "maxWidth": "1200px",
        "padding": "32px",
        "background": "#242424"
      },
      "container": {
        "padding": "0 24px",
        "margin": "0 auto"
      }
    },
    "components": {
      "navigation": {
        "item": {
          "padding": "8px 12px",
          "borderRadius": "4px",
          "fontSize": "14px",
          "transition": "background-color 0.2s ease",
          "states": {
            "default": {
              "background": "transparent",
              "color": "#b3b3b3"
            },
            "hover": {
              "background": "#333333",
              "color": "#ffffff"
            },
            "active": {
              "background": "#333333",
              "color": "#ffffff",
              "fontWeight": 500
            }
          }
        },
        "badge": {
          "beta": {
            "background": "#ff6b6b",
            "color": "#ffffff",
            "padding": "2px 6px",
            "borderRadius": "12px",
            "fontSize": "10px",
            "fontWeight": 600,
            "textTransform": "uppercase"
          },
          "new": {
            "background": "#4ecdc4",
            "color": "#ffffff",
            "padding": "2px 6px",
            "borderRadius": "12px",
            "fontSize": "10px",
            "fontWeight": 600,
            "textTransform": "uppercase"
          }
        }
      },
      "codeBlock": {
        "background": "#0d0d0d",
        "borderRadius": "6px",
        "padding": "16px",
        "fontSize": "13px",
        "fontFamily": "monospace",
        "lineHeight": 1.6,
        "border": "1px solid #333333",
        "overflow": "auto"
      },
      "button": {
        "primary": {
          "background": "#61affe",
          "color": "#ffffff",
          "padding": "8px 16px",
          "borderRadius": "4px",
          "fontSize": "14px",
          "fontWeight": 500,
          "border": "none",
          "cursor": "pointer",
          "transition": "all 0.2s ease",
          "hover": {
            "background": "#4e90d9",
            "transform": "translateY(-1px)"
          }
        },
        "secondary": {
          "background": "transparent",
          "color": "#61affe",
          "padding": "8px 16px",
          "borderRadius": "4px",
          "fontSize": "14px",
          "fontWeight": 500,
          "border": "1px solid #61affe",
          "cursor": "pointer",
          "transition": "all 0.2s ease",
          "hover": {
            "background": "rgba(97, 175, 254, 0.1)",
            "borderColor": "#4e90d9"
          }
        },
        "icon": {
          "width": "40px",
          "height": "40px",
          "borderRadius": "8px",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "center",
          "background": "#2d2d2d",
          "border": "1px solid #404040",
          "transition": "all 0.2s ease",
          "hover": {
            "background": "#333333",
            "borderColor": "#61affe"
          }
        }
      },
      "httpMethod": {
        "get": {
          "background": "#61affe",
          "color": "#ffffff",
          "padding": "4px 12px",
          "borderRadius": "4px",
          "fontSize": "12px",
          "fontWeight": 600,
          "textTransform": "uppercase"
        },
        "post": {
          "background": "#49cc90",
          "color": "#ffffff",
          "padding": "4px 12px",
          "borderRadius": "4px",
          "fontSize": "12px",
          "fontWeight": 600,
          "textTransform": "uppercase"
        },
        "put": {
          "background": "#fca130",
          "color": "#ffffff",
          "padding": "4px 12px",
          "borderRadius": "4px",
          "fontSize": "12px",
          "fontWeight": 600,
          "textTransform": "uppercase"
        },
        "delete": {
          "background": "#f93e3e",
          "color": "#ffffff",
          "padding": "4px 12px",
          "borderRadius": "4px",
          "fontSize": "12px",
          "fontWeight": 600,
          "textTransform": "uppercase"
        }
      },
      "section": {
        "header": {
          "marginBottom": "24px",
          "borderBottom": "1px solid #404040",
          "paddingBottom": "16px"
        },
        "title": {
          "fontSize": "24px",
          "fontWeight": 600,
          "color": "#ffffff",
          "marginBottom": "8px"
        },
        "subtitle": {
          "fontSize": "16px",
          "color": "#b3b3b3",
          "lineHeight": 1.6
        }
      },
      "card": {
        "background": "#2d2d2d",
        "borderRadius": "8px",
        "padding": "24px",
        "border": "1px solid #404040",
        "marginBottom": "16px"
      },
      "input": {
        "background": "#1a1a1a",
        "color": "#ffffff",
        "border": "1px solid #404040",
        "borderRadius": "4px",
        "padding": "8px 12px",
        "fontSize": "14px",
        "transition": "border-color 0.2s ease",
        "focus": {
          "borderColor": "#61affe",
          "outline": "none"
        }
      },
      "link": {
        "color": "#61affe",
        "textDecoration": "none",
        "transition": "color 0.2s ease",
        "hover": {
          "color": "#4e90d9",
          "textDecoration": "underline"
        }
      },
      "clientLibrary": {
        "container": {
          "display": "flex",
          "gap": "12px",
          "marginTop": "16px"
        },
        "item": {
          "display": "flex",
          "flexDirection": "column",
          "alignItems": "center",
          "gap": "4px",
          "cursor": "pointer"
        },
        "icon": {
          "width": "48px",
          "height": "48px",
          "borderRadius": "8px",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "center",
          "transition": "transform 0.2s ease",
          "hover": {
            "transform": "scale(1.05)"
          }
        },
        "label": {
          "fontSize": "12px",
          "color": "#b3b3b3"
        }
      },
      "authentication": {
        "container": {
          "background": "#2d2d2d",
          "borderRadius": "8px",
          "padding": "16px",
          "marginTop": "24px"
        },
        "header": {
          "fontSize": "12px",
          "textTransform": "uppercase",
          "color": "#808080",
          "marginBottom": "12px",
          "letterSpacing": "0.5px"
        },
        "row": {
          "display": "flex",
          "justifyContent": "space-between",
          "alignItems": "center",
          "marginBottom": "8px"
        },
        "label": {
          "fontSize": "14px",
          "color": "#b3b3b3"
        },
        "value": {
          "fontSize": "14px",
          "color": "#ffffff"
        }
      }
    },
    "animations": {
      "transition": {
        "fast": "0.15s ease",
        "normal": "0.2s ease",
        "slow": "0.3s ease"
      },
      "hover": {
        "scale": "scale(1.02)",
        "lift": "translateY(-2px)"
      }
    },
    "shadows": {
      "sm": "0 1px 2px rgba(0, 0, 0, 0.5)",
      "md": "0 4px 6px rgba(0, 0, 0, 0.4)",
      "lg": "0 10px 15px rgba(0, 0, 0, 0.3)",
      "xl": "0 20px 25px rgba(0, 0, 0, 0.25)"
    },
    "borderRadius": {
      "sm": "4px",
      "md": "6px",
      "lg": "8px",
      "xl": "12px",
      "full": "9999px"
    },
    "breakpoints": {
      "sm": "640px",
      "md": "768px",
      "lg": "1024px",
      "xl": "1280px",
      "2xl": "1536px"
    },
    "z-index": {
      "dropdown": 1000,
      "modal": 2000,
      "tooltip": 3000,
      "notification": 4000
    }
  }